1)#include <iostream>
using namespace std;

int main() {
    int n, sum = 0;
    cin >> n;
    for (int i = 1; i <= n; i++) {
        if (i % 2 == 0) sum += i * i;
    }
    cout << sum;
}


2)#include <iostream>
using namespace std;

int main() {
    int n, count = 0;
    cin >> n;
    if (n == 0) count = 1;
    while (n != 0) {
        count++;
        n /= 10;
    }
    cout << count;
}



3)#include <iostream>
using namespace std;

bool sade(int x) {
    if (x < 2) return false;
    for (int i = 2; i * i <= x; i++)
        if (x % i == 0) return false;
    return true;
}

int main() {
    int n;
    cin >> n;
    for (int i = 2; i < n; i++)
        if (sade(i)) cout << i << " ";
}



4)#include <iostream>
using namespace std;

int main() {
    for (int i = 100; i <= 999; i++) {
        int x = i, sum = 0;
        while (x) {
            sum += x % 10;
            x /= 10;
        }
        if (sum == 10) cout << i << " ";
    }
}



5)#include <iostream>
using namespace std;

int main() {
    int n, count = 0;
    cin >> n;
    for (int i = 1; i <= n; i++)
        if (n % i == 0) count++;
    cout << count;
}




6)#include <iostream>
using namespace std;

int main() {
    int n, x, maxv, minv;
    double sum = 0;
    cin >> n;
    cin >> x;
    maxv = minv = x;
    sum += x;

    for (int i = 1; i < n; i++) {
        cin >> x;
        sum += x;
        if (x > maxv) maxv = x;
        if (x < minv) minv = x;
    }

    cout << "max: " << maxv << "  min: " << minv 
         << "  ortalama: " << sum / n;
}



7)#include <iostream>
#include <cmath>
using namespace std;

int main() {
    for (int i = 1; i <= 9999; i++) {
        int x = i, sum = 0;
        int len = to_string(i).size();
        while (x) {
            sum += pow(x % 10, len);
            x /= 10;
        }
        if (sum == i) cout << i << " ";
    }
}



8)#include <iostream>
using namespace std;

int main() {
    int n; 
    cin >> n;
    string bin = "";
    if (n == 0) bin = "0";
    while (n > 0) {
        bin = char('0' + n % 2) + bin;
        n /= 2;
    }
    cout << bin;
}



9)#include <iostream>
using namespace std;

bool pal(int n) {
    int x = n, t = 0;
    while (x) {
        t = t*10 + x%10;
        x /= 10;
    }
    return t == n;
}

int main() {
    for (int i = 1; i <= 1000; i++)
        if (pal(i)) cout << i << " ";
}




10)#include <iostream>
using namespace std;

int main() {
    char op;
    double a, b;
    cin >> a >> op >> b;

    if (op == '+') cout << a + b;
    else if (op == '-') cout << a - b;
    else if (op == '*') cout << a * b;
    else if (op == '/') cout << a / b;
}



11)#include <iostream>
using namespace std;

int main() {
    int a = 0, b = 1;
    while (a < 1000) {
        cout << a << " ";
        int c = a + b;
        a = b;
        b = c;
    }
}



12)#include <iostream>
using namespace std;

int main() {
    int n, sum = 0;
    cin >> n;
    for (int i = 1; i <= n/2; i++)
        if (n % i == 0) sum += i;
    cout << (sum == n ? 1 : 0);
}



13)#include <iostream>
using namespace std;

int main() {
    int n, x;
    cin >> n;
    int tek = 0, cut = 0;
    for (int i = 0; i < n; i++) {
        cin >> x;
        if (i % 2 == 0) cut += x;
        else tek += x;
    }
    cout << "tek: " << tek << "  cut: " << cut;
}



14)#include <iostream>
using namespace std;

int ters(int n) {
    int t = 0;
    while (n) {
        t = t*10 + n%10;
        n /= 10;
    }
    return t;
}

int main() {
    int n;
    cin >> n;
    cout << (n == ters(n) ? 1 : 0);
}



15)#include <iostream>
using namespace std;

int main() {
    int x, maxv = -1000000000;
    while (true) {
        cin >> x;
        if (x == 0) break;
        if (x > maxv) maxv = x;
    }
    cout << maxv;
}
